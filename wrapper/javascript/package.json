	},
	"license": "MIT",
	"engines": {
		"node": "^20.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Totto16/imdb-dataset-parser.git"
	},
	"dependencies": {
		"bindings": "^1.5.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"jest": "^29.7.0",
		"nan": "^2.19.0",
		"ts-jest": "^29.1.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5"
	}
