		"build:debug": "npm run configure && node-gyp build --debug",
		"compile": "npm run build:tsc",
		"build:tsc": "tsc",
		"test": "npx jest",
		"build:test": "npm run build && npm run test"
	},
	"install": "node-gyp configure build",
	"keywords": [
		"oopetris",
		"cpp",
		"noe-gyp"
	],
	"author": {
		"name": "Totto16",
