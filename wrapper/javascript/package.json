{
	"name": "oopetris",
	"version": "1.0.0",
	"description": "Node js wrapper for oopetris",
	"gypfile": true,
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"private": false,
	"scripts": {
		"install": "npm run build",
		"configure": "node-gyp clean && node-gyp configure",
		"build": "npm run build:gyp",
